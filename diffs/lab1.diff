+        delete this->bm;
+        this->bm = NULL;
+    }
+    for (int i = 0; i < MAX_SEMAPHORE; i++) {
+        if (this->semTab[i]) {
+            delete this->semTab[i];
+            this->semTab[i] = NULL;
+        }
