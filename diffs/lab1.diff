+++ b/usr/local/nachos/lib/gcc-lib/decstation-ultrix/2.95.2/include/varargs.h
@@ -0,0 +1,229 @@
+/* Record that this is varargs.h; this turns off stdarg.h.  */
+
+#ifndef _VARARGS_H
+#define _VARARGS_H
+
+#ifdef __sparc__
+#include "va-sparc.h"
+#else
+#ifdef __spur__
+#include "va-spur.h"
+#else
+#ifdef __mips__
