+        if (bm->Test(i)) {
+            // Neu co thi tien hanh so sanh name voi name cua semaphore trong
+            // semTab
+            if (strcmp(name, semTab[i]->GetName()) == 0) {
+                // Neu ton tai thi cho semaphore up();
+                semTab[i]->signal();
+                return 0;
+            }
+        }
+    }
+    printf("Khong ton tai semaphore");
+    return -1;
+}
